      procedure MyWindow(gpclContactForm is CPresenterContactForm dynamic <presenter>, gbNewproduct is boolean = False)
      
      // By default, the window is not validated
      MyWindow.ReturnedValue = False
   -
     code : |1+
      // New or modification? 
      if gbNewproduct then
      BTN_Modify.Caption = "Add"
      end
      
      // Refresh the title of the window
      MySelf.Title=gpclContactForm.p_sFullName
      
      // Assignment
      EDT_Email = gpclContactForm.p_Email
      EDT_FirstName = gpclContactForm.p_FirstName
      EDT_Name = gpclContactForm.p_LastName
      
      // Photo available? 
      if gpclContactForm.p_Photo <> "" then	
      IMG_Profile = gpclContactForm.p_Photo
      end
      
      // Refresh the linked controls of the window
      SourceToScreen()
